<?xml version="1.0"?>
<robot name="basic_autobot">
	<link name="base_link">
		<visual>
			<geometry>
				<box size="0.3 0.2 0.2"/>
			</geometry>
		</visual>
	</link>
	<link name="laser_link">
		<visual>
			<geometry>
				<box size="0.1 0.1 0.1"/>
			</geometry>
		</visual>
	</link>
	<joint name="laser_joint" type="fixed">
	        <parent link="base_link"/>
	        <child link="laser_link"/>
	        <origin xyz="0 0 0.2" rpy="0 0 0"/>
	</joint>
</robot>
